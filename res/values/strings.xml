    <string name="about">About</string>

    <!-- Do not translate. Duration format for duration < 1 hour -->
    <string translatable="false" name="durationformatshort"><xliff:g id="format">%2$d:%5$02d</xliff:g></string>
    <!-- Do not translate. Duration format for duration >= 1 hour -->
    <string translatable="false" name="durationformatlong"><xliff:g id="format">%1$d:%3$02d:%5$02d</xliff:g></string>

    <!-- Dialog box title -->
    <string name="service_start_error_title">Playback problem</string>
    <!-- Dialog box message -->
    <string name="service_start_error_msg">Sorry, the song could not be played.</string>
    <!-- Dialog box button -->
    <string name="service_start_error_button">OK</string>

    <!-- About dialog -->
    <string name="sexypsf_about">MinePsfPlayer v%1$s\n\n
        This is an open sourced music player for Sony PlayStation Video Game\'s original sound track.\n
        \n
        Thanks to Raphael Assenat for his GPL sexypsf lib.\n
        Visit http://projects.raphnet.net for more information about sexypsf project.\n\n
        To download psf files, google \"psf download\" or use this mirror:\n
        http://tzone.org/~llin/psf
    </string>

    <!-- Preference strings -->
    <string name="pref_settings">Settings</string>
    <!-- string name="pref_setting_category_psf">Player Settings</string -->
    <string name="pref_psf_root_dir_title">Psf Directory</string>
    <string name="pref_psf_root_dir_dialog_title">Set Psf Root Directory</string>
    <string name="first_time_run_dialog_msg">
        This may be the first time you\'ve run this app.\n
        Please set the directory where your PSF files are held.\n
        The default is /mnt/sdcard (external storage).</string>
        
    <string name="pref_email_title">Feedback (email)</string>
    <string name="pref_email_summary">Issues, Suggestions? Send me email~</string>

    <!-- Toast strings -->
    <string name="playlist_empty_error">Playlist Empty!\nPlease select a direcotry contains psf files.</string>
